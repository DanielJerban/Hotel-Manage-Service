    }

    #monthName {
        text-align: center;
        color: #d33e30;
        font-size: 30px;
        margin-bottom: 0;
    }

    #parent-flex-calender-week {
        margin-bottom: 10px !important;
    }

    .calender-header {
        background-color: #494f5c;
